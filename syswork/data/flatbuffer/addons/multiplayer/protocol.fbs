namespace Multiplayer.Protocol;

table MsgTest
{
    value: float;
}

table MsgConnected
{
    // Empty message
}

union Data
{
    Test: MsgTest,
    Connected: MsgConnected
}


table Message
{
    data: Data;
}

root_type Message;
